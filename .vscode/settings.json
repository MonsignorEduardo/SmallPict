{
  "deno.enable": true,
  "deno.lint": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "css.customData": [".vscode/tailwind.json"],
  "cSpell.words": [
    "daisyui",
    "jsquash",
    "magick",
    "Magick",
    "mozjpeg",
    "preact",
    "squoosh",
    "tailwindcss",
    "Vips",
    "webp"
  ],
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
